<div class="card card-primary">
  <form action="/admin/module-documents/add" method="post">
    <div class="card-body">
      <div class="form-group">
        <label for="">Nội dung tài liệu:</label>
        <input type="text" class="form-control" name="documentContent" placeholder="Nhập tiêu đề tài liệu" />
      </div>
      <div class="form-group">
        <label for="">Đường dẫn tới tài liệu:</label>
        <input type="text" class="form-control" name="pathName" placeholder="Nhập đường dẫn tới tài liệu" />
      </div>
      <div class="form-group">
        <label for="">Tài liệu:</label>
        <select class="form-control" name="moduleId">
          <option value="">Chọn tài liệu</option>
          <% courseModule.forEach((moduleVal) => { %>
          <option value="<%= moduleVal.id %>"><%= moduleVal.name %> - <%= moduleVal.Course.name %></option>
          <% }) %>
        </select>
      </div>
    </div>
    <div class="card-footer">
      <button type="submit" class="btn btn-primary">
        Thêm tài liệu
      </button>
    </div>
  </form>
</div>